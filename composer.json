{
	"name": "extension/plugin-name",
	"description": "A WooCommerce extension plugin boilerplate",
	"autoload": {
		"psr-4": {
			"SSEC\\": "includes/"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^9.0",
		"automattic/jetpack-autoloader": "^2",
		"wp-coding-standards/wpcs": "^3.3"
	},
	"config": {
		"allow-plugins": {
			"automattic/jetpack-autoloader": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}