{
	"name": "plugin-name",
	"version": "0.1.0",
	"author": "Shohei Tanaka",
	"license": "GPL-3.0+",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build --blocks-manifest",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start --blocks-manifest",
		"postinstall": "composer install",
		"make-pot": "wp-env run cli --env-cwd=wp-content/plugins/saai*/ wp i18n make-pot ./ i18n/saai*.pot --ignore-domain",
		"make-json": "wp-env run cli --env-cwd=wp-content/plugins/saai*/ wp i18n make-json i18n --no-purge"
	},
	"dependencies": {
		"@woocommerce/components": "latest",
		"@wordpress/hooks": "latest",
		"@wordpress/i18n": "latest"
	},
	"devDependencies": {
		"@woocommerce/dependency-extraction-webpack-plugin": "latest",
		"@woocommerce/eslint-plugin": "latest",
		"@wordpress/prettier-config": "latest",
		"@wordpress/scripts": "latest",
		"ajv": "^8.17.1"
	},
	"overrides": {
		"@wordpress/vips": "1.0.1-next.v.202602200903.0"
	}
}
